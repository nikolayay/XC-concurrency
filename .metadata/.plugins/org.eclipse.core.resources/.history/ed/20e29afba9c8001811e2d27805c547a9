#include <stdio.h>
#include <platform.h>

struct Ant {
   uint world[12];
   int food_counter;
   uint pos;
};

struct Ant ant(uint w[], int ant_id, uint ant_pos) {

    struct Ant *new_ant;

    new_ant = malloc(sizeof(struct Ant));

    memcpy(new_ant->world, w, 12 * sizeof(uint));
    new_ant->food_counter = 0;
    new_ant->pos = ant_pos;

    return *new_ant;

}




