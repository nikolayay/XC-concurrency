#include <stdio.h>
#include <platform.h>

struct Ant {
   uint world[12];
   int  food_counter;
   int  pos;
} ant;

struct Ant makeAnt(uint world[], int ant_id, int ant_pos) {

    struct Ant *new_ant;

    new_ant = malloc(sizeof(struct Ant));
    new_ant->world = world;
    new_ant->food_counter = 0;
    new_ant->pos = ant_pos;

    return *new_ant;

}




