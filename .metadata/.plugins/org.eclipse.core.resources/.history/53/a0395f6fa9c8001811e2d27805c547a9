#include <stdio.h>
#include <partc.c>
#include <platform.h>


extern void ant(uint world[], int ant_id, int ant_pos);

int main(void) {

    const uint world[12] = {10, 0, 1, 7, 2, 10, 0, 3, 6, 8, 7, 6};

    par {
        ant(world, 0, 1);
    }
    return 0;

}
