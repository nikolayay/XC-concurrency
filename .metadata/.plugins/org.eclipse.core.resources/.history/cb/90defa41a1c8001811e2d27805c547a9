#include <stdio.h>
#include <platform.h>

void hello(int a);


int main(void) {
    par {
        on tile[0] : hello(0);
        on tile[1] : hello(1);

    }

    return 0;
}

void hello(int a) {
    printf("Hello from tile #%d.\n", a);

}
